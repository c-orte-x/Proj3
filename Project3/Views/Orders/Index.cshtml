                @Html.DisplayNameFor(model => model.ShippingCost)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OrderPriority)
            </th>
            <th></th>
        </tr>
    </thead> 
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RowID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OrderID)
