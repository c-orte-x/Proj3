}

<h1>Order Gallery</h1>

<p>
    <a asp-action="Create">Add New Orders</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RowID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OrderID)
